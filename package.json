{
  "name": "power-copy",
  "private": true,
  "version": "0.0.0",
  "author": "Yorn Qiu",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "tauri": "tauri"
  },
  "dependencies": {
    "@tauri-apps/api": "^1.2.0",
    "vue": "^3.2.47",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^1.2.3",
    "@types/node": "^18.13.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.32.0",
    "eslint-plugin-vue": "^9.9.0",
    "prettier": "^2.8.3",
    "typescript": "^4.9.5",
    "unplugin-auto-import": "^0.14.2",
    "vite": "^4.1.1",
    "vue-tsc": "^1.0.24"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended",
      "@vue/eslint-config-typescript/recommended",
      "@vue/eslint-config-prettier"
    ],
    "env": {
      "node": true,
      "vue/setup-compiler-macros": true
    },
    "rules": {
      "no-undef": "off",
      "vue/multi-word-component-names": "off"
    },
    "ignorePatterns": [
      "auto-imports.d.ts"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 120
  }
}
